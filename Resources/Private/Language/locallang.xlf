<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.0" xmlns:t3="http://typo3.org/schemas/xliff">
	<file source-language="en" datatype="plaintext" original="messages" product-name="move">
		<header/>
		<body>
			<trans-unit id="moveTask.name">
				<source>Move or Copy Task</source>
			</trans-unit>
			<trans-unit id="moveTask.description">
				<source>The move task reads various database records from a table, changes their page id and puts the results back into the database. The control is based on a XML file. The records can either be copied or moved.</source>
			</trans-unit>
			<trans-unit id="moveTables">
				<source>Tables for Move or Copy</source>
			</trans-unit>
            <trans-unit id="configurationFile" xml:space="preserve">
                <source>Configuration File</source>
            </trans-unit>
            <trans-unit id="options">
                <source>Options to choose</source>
            </trans-unit>
            <trans-unit id="options_move" xml:space="preserve">
                <source>Move records</source>
            </trans-unit>
            <trans-unit id="options_copy" xml:space="preserve">
                <source>Copy records</source>
            </trans-unit>
            <trans-unit id="error_empty_configurationFile" xml:space="preserve">
                <source>You must enter the Configuration File!</source>
            </trans-unit>
            <trans-unit id="error_empty_tables" xml:space="preserve">
                <source>You must select at least one table name!</source>
            </trans-unit>
            <trans-unit id="error_internal" xml:space="preserve">
                <source>An internal error has happened!</source>
            </trans-unit>
            <trans-unit id="info_copied">
                <source>The records with these uids have been generated by a copy of records from table %s: %s</source>
            </trans-unit>
		</body>
	</file>
</xliff>


